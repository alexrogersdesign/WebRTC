{
  "ts-node": {
    "moduleTypes" :{
      "wdio.conf.ts": "cjs",
      "test": "cjs",
      "test/**/*": "cjs",
      "example.e2e.ts": "cjs"
    },
    "compilerOptions": { "module": "commonjs" }
  },
    "compileOnSave": true,
  "compilerOptions": {
    "target": "es2015",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "es6"
    ],
    "types": [
      "node",
      "webdriverio/async",
      "@wdio/mocha-framework",
      "expect-webdriverio",
      "mocha"
    ],
    "module": "ESNext"
  },
//  "extends": "../tsconfig.json.old",
  "files": ["wdio.conf.ts", "example.e2e.ts"],
  "include": [
    "src/test",
    "./src/test/**/*.ts",
    "./src/test/**/*"
  ],
  "exclude": [
    "src",
    "./src/**",
    "./src/test/**/*.js",
    "./src/test/pageObjects"
  ]

}
